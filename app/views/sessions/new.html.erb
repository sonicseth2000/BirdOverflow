<h1>New session</h1>

<h1>Log in</h1>  
  
<%= form_tag sessions_path do %>  
  <div class="field">  
    <%= label_tag :username %>  
    <%= text_field_tag :username, params[:username] %>  
  </div>  
  <div class="field">  
    <%= label_tag :password %>  
    <%= password_field_tag :password %>  
  </div>  
  <div class="actions"><%= submit_tag "Log in" %></div>  
<% end %> 
<%= link_to 'New User', new_user_path %>
<%= link_to 'Back', sessions_path %>
